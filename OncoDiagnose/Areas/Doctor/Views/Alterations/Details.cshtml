@model OncoDiagnose.Web.ViewModels.AlterationViewModel

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<br />
<div class="container">
    <div class="row p-3 border">
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <div class="col-12 border-bottom">
            <h2 class="text-primary">Detail</h2>
        </div>
        <div class="col-8 pt-4">
            <div class="form-group row">
                <div class="col-4">
                    <label asp-for="AlterationInformation"></label>
                </div>
                <div class="col-8">
                    @Html.DisplayFor(item => Model.AlterationInformation)
                </div>
            </div>

            <div class="form-group row">
                <div class="col-4">
                    <label asp-for="RefResidues"></label>
                </div>
                <div class="col-8">
                    @Html.DisplayFor(item => Model.RefResidues)
                </div>
            </div>
            <div class="form-group row">
                <div class="col-4">
                    <label asp-for="VariantResidues"></label>
                </div>
                <div class="col-8">
                    @Html.DisplayFor(item => Model.VariantResidues)
                </div>
            </div>
            <div class="form-group row">
                <div class="col-4">
                    <label asp-for="ProteinStart"></label>
                </div>
                <div class="col-8">
                    @Html.DisplayFor(item => Model.ProteinStart)
                </div>
            </div>

            <div class="form-group row">
                <div class="col-4">
                    <label asp-for="ProteinEnd"></label>
                </div>
                <div class="col-8">
                    @Html.DisplayFor(item => Model.ProteinEnd)
                </div>
            </div>

            <div class="form-group row">
                <div class="col-4">
                    <label asp-for="Gene.HugoSymbol"></label>
                </div>
                <div class="col-8">
                    @Html.DisplayFor(item => Model.Gene.HugoSymbol)
                </div>
            </div>

            <div class="form-group row">
                <div class="col-4">
                    <label asp-for="Consequence.Description"></label>
                </div>
                <div class="col-8">
                    @Html.DisplayFor(item => Model.Consequence.Description)
                </div>
            </div>

            <div class="form-group row">
                <div class="col-4">
                    <label asp-for="MutationId"></label>
                </div>
                <div class="col-8">
                    @Html.DisplayFor(item => Model.MutationId)
                </div>
            </div>
        </div>
    </div>
    <br/>
    <div>
        <a class="btn btn-primary" href="/Doctor/Mutations">Back to list</a>
    </div>
</div>
<br />
